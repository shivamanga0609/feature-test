pipeline {
    agent any 
    stages {
        stage('git repo & clean') { 
            steps {
                bash "rm -r feature-test"
                bash " git clone https://github.com/shivamanga0609/feature-test.git"
                bash "mvn clean -f  feature-test"
            }
        }
        stage('install') { 
            steps {
                bash "mvn install -f feature-test"
            }
        }
        stage('test') { 
            steps {
                bash "mvn test -f  feature-test"
            }
        }
        stage(package') {
            steps {
                bash "mvn package -f feature-test"
            }
        }
    }
}
